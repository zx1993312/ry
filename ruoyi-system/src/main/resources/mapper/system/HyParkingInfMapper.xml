	<sql id="selectHyParkingInfVo">
		select 
		p.id, 
		p.parking_area, 
		p.parking_type, 
		p.parking_class, 
		p.parking_number,
		p.contract_delivery_date, 
		p.actual_delivery_date, 
		p.takeover_time,
		p.charging_standard, 
		p.current_state, 
		p.parking_card, 
		p.license_plate,
		p.vehicle_type, 
		p.vehicle_brand, 
		p.displacement, 
		p.vehicle_color,
		p.other_expenses, 
		p.license_plate_not, 
		p.license_plate_time, 
		p.usage_status,
		p.parking_property, 
		p.time_processing, 
		p.time_str, 
		p.time_end,
		p.biling_cycle,
		p.parking_name,
		p.owner_id, 
		p.house_id, 
		o.owner_name, 
		h.house_number, 
		h.house_name 
		from 
		hy_parking_inf p
		LEFT JOIN hy_owner_registration o ON p.owner_id =o.id
		LEFT JOIN hy_house_inf h ON p.house_id =h.id
	</sql>

