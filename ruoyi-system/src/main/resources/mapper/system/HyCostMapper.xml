	<sql id="selectHyCostVo">
		SELECT
		c.id,
		c.cost_items,
		c.expense_type,
		c.parent_id,
		c.ancestors,
		c.standard_name,
		c.advance_payment_offset,
		c.charging_standard,
		c.calculation_mehod,
		c.standard_description,
		c.standard_state,
		c.proportion_damags,
		c.project_modification,
		c.biling_start_date,
		c.biling_end_date,
		c.audit_results_reason,
		c.fee_date_old,
		c.fee_date,
		c.due_date_old,
		c.due_date,
		c.amount_receivable_old,
		c.amount_receivable,
		c.amount_actually,
		c.remarks,
		c.operation_date,
		c.operation_user,
		c.operation_type,
		c.delete_reason,
		c.advance_payment_offset,
		c.write_off,
		c.refund_amount,
		c.amount_arrears,
		c.collection_status,
		c.frozen_person,
		c.frozen_state,
		c.frozen_date,
		c.frozen_reason,
		c.advance_status,
		c.billing_cycle,
		c.transfer_tenants,
		c.change_date,
		c.billing_method,
		c.calculation_standard,
		o.owner_name,
		h.house_name,
		h.house_number,
		h.delivery_status,
		p.parking_number,
		m.meter_name,
		a.owner_id
		FROM
		hy_cost c
		LEFT JOIN owner_and_cost a ON c.id = a.cost_id
		LEFT JOIN hy_owner_registration o ON a.owner_id = o.id
		LEFT JOIN hy_house_inf h ON h.owner_id = o.id
		LEFT JOIN hy_meter m ON m.house_id = h.id
		LEFT JOIN hy_parking_inf p ON p.owner_id = o.id
		WHERE
		1 = 1
	</sql>

	<sql id="selectHyCostDistinctVo">
		SELECT
		DISTINCT c.cost_items,c.id
		FROM
		hy_cost c
		WHERE
		1=1
	</sql>

