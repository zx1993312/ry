	<sql id="selectHyHouseInfVo">
		select 
		h.id, 
		h.house_number, 
		h.house_name, 
		h.built_up_area, 
		h.group_area,
		h.unit, 
		h.floor, 
		h.same_unit_number, 
		h.area_type, 
		h.biling_area, 
		h.inside_area,
		h.shared_area, 
		h.garden_area, 
		h.basement_area, 
		h.shared_lobby_area,
		h.shared_layers_area, 
		h.share_ratio, 
		h.building_structure, 
		h.parameters_per,
		h.house_layout, 
		h.floor_height, 
		h.nature_property, 
		h.nature_use, 
		h.usage_status,
		h.occupancy_status,
		h.delivery_status_before, 
		h.delivery_status, 
		h.change_date,
		h.change_person,
		h.remark, 
		h.house_type,
		h.sales_status,
		h.purchase_contract, 
		h.marketing_num, 
		h.building_id, 
		h.owner_id, 
		b.building_name, 
		o.owner_name 
		from 
		hy_house_inf h
		LEFT JOIN hy_building b ON h.building_id =b.id
		LEFT JOIN hy_owner_registration o ON h.owner_id =o.id
	</sql>
	
	<sql id="selectHyHouseInfVoDistinct">
		select 
		DISTINCT unit
		from 
		hy_house_inf
	</sql>
	
	<sql id="selectHyHouseInfVoDistinctByhouseNumber">
