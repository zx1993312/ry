    <sql id="selectHyMeterVo">
        select id, house_num, meter_type, meter_serial_num, meter_name, initial_read, meter_str_num, meter_end_num, transf_ratio, reverse_not, strapping_type, meter_read_date, magnification, new_meter_consumption, old_meter_consumption, attrition_rate, unit_price, amount_price, abnormal_prompt, customer_name, freeze_not, source_meter, meter_reader, input_time from hy_meter
    </sql>

    <select id="selectHyMeterList" parameterType="HyMeter" resultMap="HyMeterResult">
        <include refid="selectHyMeterVo"/>
        <where>  
            <if test="houseNum != null  and houseNum != ''"> and house_num = #{houseNum}</if>
            <if test="meterType != null "> and meter_type = #{meterType}</if>
            <if test="meterSerialNum != null "> and meter_serial_num = #{meterSerialNum}</if>
            <if test="meterName != null  and meterName != ''"> and meter_name like concat('%', #{meterName}, '%')</if>
            <if test="initialRead != null "> and initial_read = #{initialRead}</if>
            <if test="meterStrNum != null "> and meter_str_num = #{meterStrNum}</if>
