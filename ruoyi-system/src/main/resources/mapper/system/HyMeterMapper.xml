            <if test="attritionRate != null  and attritionRate != ''"> and attrition_rate = #{attritionRate}</if>
            <if test="unitPrice != null "> and unit_price = #{unitPrice}</if>
            <if test="amountPrice != null "> and amount_price = #{amountPrice}</if>
            <if test="abnormalPrompt != null  and abnormalPrompt != ''"> and abnormal_prompt = #{abnormalPrompt}</if>
            <if test="customerName != null  and customerName != ''"> and customer_name like concat('%', #{customerName}, '%')</if>
            <if test="freezeNot != null "> and freeze_not = #{freezeNot}</if>
            <if test="sourceMeter != null  and sourceMeter != ''"> and source_meter = #{sourceMeter}</if>
            <if test="meterReader != null  and meterReader != ''"> and meter_reader = #{meterReader}</if>
            <if test="inputTime != null "> and input_time = #{inputTime}</if>
        </where>
    </select>
    
    <select id="selectHyMeterById" parameterType="Long" resultMap="HyMeterResult">
